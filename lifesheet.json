    "mood": {
        "description": "Track my current mood during the day",
        "schedule": "eightTimesADay",
        "questions": [
            {
                "key": "happyLevels",
                "displayName": "Happiness",
                "question": "How happy/excited are you feeling right now?",
                "type": "range",
                "isVisibleInVisualizer": true,
                "category": "Mental Health",
                "buttons": {
                    "0": "Extremely sad/dull",
                    "1": "Very sad/dull",
                    "2": "Sad/dull",
                    "3": "Moderate",
                    "4": "Happy/excited",
                    "5": "Very happy/excited"
                }
            },
            {
                "key": "distractionLevels",
                "displayName": "Distracted",
                "question": "How distracted are you feeling right now?",
                "type": "range",
                "isPositive": false,
                "isReverse": true,
                "isVisibleInVisualizer": true,
                "category": "Mental Health",
                "buttons": {
                    "0": "Cant concentrate for even 2 mins.",
                    "1": "Can work but feel the need for some distraction",
                    "2": "Tough, but can do pomodoro",
                    "3": "Easily do 25 mins",
                    "4": "Can concentrate for a good 1.5 hours.",
                    "5": "Can't stop myself from working"
                }
            },
            {
                "key": "anxietyLevels",
                "displayName": "Anxiety",
                "question": "How anxious/stressed are you feeling right now?",
                "isVisibleInVisualizer": true,
                "isPositive": false,
                "isReverse": true,
                "category": "Mental Health",
                "type": "range",
                "buttons": {
                    "0": "Extremely",
                    "1": "Very much",
                    "2": "Quite a bit",
                    "3": "Moderate",
                    "4": "A little",
                    "5": "Not at all"
                }
            },
            {
                "key": "energyLevels",
                "displayName": "Energy",
                "question": "How much energy do you have right now?",
                "isVisibleInVisualizer": true,
                "category": "Mental Health",
                "type": "range",
                "buttons": {
                    "0": "Extremely low",
                    "1": "Very low",
                    "2": "Low",
                    "3": "Moderate",
                    "4": "High",
                    "5": "Very high"
                }
            }
        ]
    },
